    Public Shared Property User() As UserVM
        Get

            Return Session(_USER)
        End Get
        Set(ByVal usr As UserVM)

            Session(_USER) = usr
            'Session(_USER_ROLE) = usr.Roles
        End Set
    End Property

    Public Shared ReadOnly Property IsConnect() As Boolean
        Get
            Return User IsNot Nothing
        End Get

    End Property

